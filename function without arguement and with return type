#include<stdio.h>
int sum();
int main()
{
    int res;
    res=sum();
    printf("the result is %d",res);
    res=sum();
    printf("the result is%d",res);
    return 0;
}
int sum()
{

    int a,b;
    printf("\n enter any two numbers");
    scanf("%d%d",&a,&b);
    return a+b;
}
